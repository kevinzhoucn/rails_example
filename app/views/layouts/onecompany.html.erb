<!DOCTYPE html>
<html>
<head>
  <title>RailsExamples</title>  
  <link href="<%= root_url %>css/bootstrap.min.css" rel="stylesheet">
  <link href="<%= root_url %>css/bootstrap-responsive.min.css" rel="stylesheet">
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= csrf_meta_tags %>
</head>

<body role="document">
  <div class="row">
    <div class="container-fixed-navbar" data-desc="fixed-navbar">
      <%= render :partial => 'onecompany/common/top_nav' %>
    </div>    
    <div class="container" >
      <%= yield %>
    </div>
    <div class="container">
      <footer>
        <%= render :partial => 'onecompany/common/footer' %>
      </footer>
    </div>
  </div>

  <!-- Bootstrap core JavaScript
  ================================================== -->
  <!-- Placed at the end of the document so the pages load faster -->
  <script src="<%= root_url %>js/jquery-1.11.0.min.js"></script>  
  <script src="<%= root_url %>js/bootstrap.min.js"></script>
  <%= javascript_include_tag "application" %>
</body>
</html>