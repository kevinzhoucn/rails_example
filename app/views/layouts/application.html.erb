    </div>
    <div class="row" data-desc="content">
      <%#= render :partial => 'common/theme/content' %>
    </div>
  </div>
--> 

  <div class="container">
    <footer>
      <%= render :partial => 'common/theme/footer' %>
    </footer>
  </div>
  <!-- Bootstrap core JavaScript
